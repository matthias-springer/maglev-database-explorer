<hr style="margin: 10px;"/>

<table style="margin-bottom: 0px;" class="table table-bordered">
  <tbody>
    <% (0..object.length - 1).each do |index| %>
      <tr>
        <td>
          <%= index %>
        </td>
        <td>
          <%= render(:partial => "draggable_new_object_begin", :locals => {:id => object[index].object_id}) %>
            <div class="object-drag-dummy" style="display: none;">
              <b class="icon-move"></b>
              <%= render(:partial => "shared/object", :locals => {:object => object[index]}) %>
            </div>
            <div class="object-iv-name">
              <%= render(:partial => "shared/object", :locals => {
                :object => object[index],
                :object_owner => object.object_id,
                :object_setter_ruby => "self[#{index}] = "}) %>
            </div>
          <%= render(:partial => "draggable_new_object_end", :locals => {:id => object[index].object_id}) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
