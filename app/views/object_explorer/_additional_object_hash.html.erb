<hr style="margin: 10px;"/>

<table style="margin-bottom: 0px;" class="table table-bordered">
  <tbody>
    <% object.each do |key, value| %>
      <tr>
        <td>
          <%= render(:partial => "draggable_new_object_begin", :locals => {:id => key.object_id}) %>
            <div class="object-drag-dummy" style="display: none;">
              <b class="icon-move"></b>
              <%= render(:partial => "shared/object", :locals => {:object => key}) %>
            </div>
            <div class="object-iv-name">
              <%= render(:partial => "shared/object", :locals => {:object => key}) %>
            </div>
          <%= render(:partial => "draggable_new_object_end", :locals => {:id => key.object_id}) %>
        </td>
        <td>
          <%= render(:partial => "draggable_new_object_begin", :locals => {:id => value.object_id}) %>
            <div class="object-drag-dummy" style="display: none;">
              <b class="icon-move"></b>
              <%= render(:partial => "shared/object", :locals => {:object => value}) %>
            </div>
            <div class="object-iv-name">
              <%= render(:partial => "shared/object", :locals => {
                :object => value,
                :object_owner => object.object_id,
                :object_setter_ruby => "self[#{key}] = "}) %>
            </div>
          <%= render(:partial => "draggable_new_object_end", :locals => {:id => value.object_id}) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
