<script type="text/javascript">
  loadAmber({
    packages: ["AmberProject-Core.js"],
    packageHome: "/amber/server/projects/importer/",
    ready: function() {
      $(function() {
        smalltalk.AmberProjectExporter._registerPackage_type_prefix_(
          'AmberProject-Core',
          'js',
          '/amber/server/projects/importer/'
        );
        smalltalk.AmberProjectImporter._importStPackages_prefix_(
          [
            'Canvas',
            'Maglev-Database-Explorer',
            'Maglev-Core',
            'Maglev-Vendor'
          ],
          '/amber/');
        smalltalk.Browser._open();
      });
    }
  }); 
</script>

